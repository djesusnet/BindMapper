    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <Authors>FlashMapper Contributors</Authors>
    <Company>FlashMapper</Company>
    <Copyright>Copyright Â© 2026 FlashMapper Contributors</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/flashmapper/flashmapper</PackageProjectUrl>
